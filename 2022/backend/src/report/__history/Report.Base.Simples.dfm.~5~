  object qryReport: TFDQuery
    SQL.Strings = (
      'select *'
      
        '  from (select dmd.id_dmd, dmd.descricao_dmd, dmd.grupo_dmd, dmd' +
        '.assunto_dmd, dmd.classassunto_dmd,'
      
        '               dmd.tipo_ate_dmd, dmd.id_aac_dmd, dmd.ativo_dmd, ' +
        'dmd.id_cnv, aac.descricao_aac,'
      '               grp.descricao_dmd as grupo'
      '          from sas_demandas dmd'
      
        '          left join sas_atendimentos_acao aac on aac.id_aac = dm' +
        'd.id_aac_dmd'
      
        '          left join sas_demandas grp on grp.id_dmd = dmd.grupo_d' +
        'md)')
    Left = 71
    Top = 54
